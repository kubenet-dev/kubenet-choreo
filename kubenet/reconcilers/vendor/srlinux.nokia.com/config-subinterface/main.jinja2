{%- import 'subinterface.jinja2' subinterface -%}
apiVersion: config.sdcio.dev/v1alpha1
kind: Config
metadata:
  name: subinterface.{{ metadata.name }}
  namespace: {{ metadata.namespace }}
  labels:
    config.sdcio.dev/targetName: {{ spec.partition }}.{{spec.region}}.{{spec.site}}.{{spec.node}}
    config.sdcio.dev/targetNamespace: {{ metadata.namespace }}
spec:
  priority: 10
  config:
  - path: /
    value: 
      {{ subinterface(spec) -}} 